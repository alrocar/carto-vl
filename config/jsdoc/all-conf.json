{
    "source": {
        "include": [
            "src"
        ]
    },
    "templates": {
        "default": {
            "outputSourceFiles": false,
            "useLongnameInNav": true
        }
    },
    "plugins": [
        "plugins/markdown",
        "node_modules/jsdoc-escape-at"
    ],
    "opts": {
        "recurse": true,
        "destination": "./docs/all"
    }
}
